<link class="cssdeck" rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.3.1/css/bootstrap.min.css">
<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.3.1/css/bootstrap-responsive.min.css" class="cssdeck">

<div class="profile_body">
  <%= render :partial => '/trips/form' %>
</div>


<div class="well" style="width:180px; padding: 8px 0;">
    <div style="overflow-y: scroll; overflow-x: hidden; height: 500px;">
        <ul class="nav nav-list">
          <% @trips.each do |trip| %>
            <div class="trip">
              <li><label class="tree-toggler nav-header"><%= trip.name %></label>
                  <ul class="nav nav-list tree">
                    <% trip.legs.each do |leg| %>
                      <li><label class="tree-toggler nav-header"><%= leg.location.name %></label>
                          <ul class="nav nav-list tree">
                              <li><a href="#">Transport</a></li>
                              <li><a href="#">Food</a></li>
                              <li><a href="#">Housing</a></li>
                              <li><a href="#">Shopping</a></li>
                              <li><a href="#">Misc</a></li>
                          </ul>
                      </li>
                  <% end %>
                </div>
          <% end %>
       <li class="divider"></li>
    </div>
</div>


<script class="cssdeck" src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
<script class="cssdeck" src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.3.1/js/bootstrap.min.js"></script>












